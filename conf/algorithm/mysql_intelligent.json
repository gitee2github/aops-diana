{
    "metric_list": {
        "gala_gopher_net_tcp_curr_estab": {
            "filter_rule": {}
        },
        "gala_gopher_cpu_net_rx": {
            "filter_rule": {}
        },
        "gala_gopher_nic_tc_backlog": {
            "filter_rule": {}
        },
        "gala_gopher_nic_rx_packets": {
            "filter_rule": {}
        }
    },
    "rule": {
        "rule1": {
            "related_metrics": {
                "gala_gopher_net_tcp_curr_estab": {
                    "preprocess_model": {
                        "enabled": "adtk",
                        "algorithm_list": {
                            "adtk": {
                                "algorithm_name": "DoubleRollingAggregate",
                                "param": {
                                    "agg": "mean",
                                    "window": 18,
                                    "diff": "diff"
                                }
                            }
                        }
                    },
                    "check_model": {
                        "enabled": "adtk",
                        "algorithm_list": {
                            "adtk": {
                                "algorithm_name": "PersistAD",
                                "param": {
                                    "window": 24,
                                    "c": 6,
                                    "side": "negative",
                                    "agg": "median"
                                }
                            }
                        }
                    }
                },
                 "gala_gopher_cpu_net_rx": {
                    "preprocess_model": {
                        "enabled": "none"
                    },
                    "check_model": {
                        "enabled": "adtk",
                        "algorithm_list": {
                            "adtk": {
                                "algorithm_name": "LevelShiftAD",
                                "param": {
                                    "window": [50, 15],
                                    "c": 3,
                                    "side": "negative"
                                }
                            }
                        }
                    }
                }
            },
            "fusion_strategy": "intersection"
        },
        "rule2": {
            "related_metrics": {
                "gala_gopher_nic_tc_backlog": {
                    "preprocess_model": {
                        "enabled": "none"
                    },
                    "check_model": {
                        "enabled": "adtk",
                        "algorithm_list": {
                            "adtk": {
                                "algorithm_name": "PersistAD",
                                "param": {
                                    "window": 20,
                                    "c": 5,
                                    "side": "positive",
                                    "agg": "median"
                                }
                            }
                        }
                    }
                },
                 "gala_gopher_nic_rx_packets": {
                    "preprocess_model": {
                        "enabled": "none"
                    },
                    "check_model": {
                        "enabled": "adtk",
                        "algorithm_list": {
                            "adtk": {
                                "algorithm_name": "ThresholdAD",
                                "param": {
                                    "high": 600
                                }
                            }
                        }
                    }
                }
            },
            "fusion_strategy": "intersection"
        }
    }
}
